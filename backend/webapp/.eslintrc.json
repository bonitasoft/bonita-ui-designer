{
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "globals": {
    "angular": false,
    "ace": false,
    "console": false,
    "ActiveXObject": false,
    "describe": false,
    "fail": false,
    "beforeEach": false,
    "module": false,
    "inject": false,
    "afterEach": false,
    "it": false,
    "xit": false,
    "expect": false,
    "Buffer": false,
    "jasmine": false,
    "spyOn": false,
    "$$": false,
    "$": false,
    "browser": false,
    "by": false,
    "element": false,
    "protractor": false,
    "require": false
  },
  "rules": {
    "no-bitwise": 2,
    "camelcase": 2,
    "curly": 2,
    "eqeqeq": 2,
    "wrap-iife": [
      2,
      "any"
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-use-before-define": [
      2,
      {
        "functions": false
      }
    ],
    "new-cap": 2,
    "no-caller": 2,
    "quotes": [
      2,
      "single"
    ],
    "no-undef": 2,
    "no-unused-vars": 2,
    "strict": 0
  }
}
